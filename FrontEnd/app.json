{
  "expo": {
    "name": "AskJC Chatbot",
    "slug": "askjc-chatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-192.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-192.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "manifest": {
        "name": "AskJC Chatbot",
        "short_name": "AskJC",
        "description": "AI-powered chatbot assistant",
        "theme_color": "#10b981",
        "background_color": "#ffffff",
        "display": "standalone",
        "orientation": "portrait",
        "scope": "/",
        "start_url": "/"
      },
      "themeColor": "#10b981"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-192.png",
        "backgroundColor": "#ffffff"
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}