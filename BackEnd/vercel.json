{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "installCommand": "npm install && npx prisma generate",
  "buildCommand": "echo 'No build needed - Vercel handles TS compilation'"
}